{
  "workflow_description": {
    "name": "single_cell",
    "version": 1.0,
    "label": "Single cell",
    "type": "single_cell",
    "inputs": "*/{sample}*fastq.gz",
    "outputs": [
      "logs/*",
      "cellranger/*",
      "singleCell_fastq/*",
      "single_cell_final_report.html"
    ],
    "report_index": "single_cell_final_report.html",
    "reports":[
       "single_cell_final_report.html"
    ]
  },
  "general_params": [
    "library_name",
    "sample_name",
    "lib_forward_read_length",
    "lib_reverse_read_length"
  ],
  "gui_params": {
    "detailed": {
      "sc_hashtags": {
        "label": "sc_hashtags",
        "type": "string",
        "default": "classic"
      }
    }
  }
}

